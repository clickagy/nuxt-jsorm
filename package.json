    "eslint-plugin-lodash-template": "^0.11.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.0.0-beta.5",
    "express": "^4.16.4",
    "jest": "^23.6.0",
    "jest-environment-node": "^23.4.0",
    "jest-serializer-vue": "^2.0.2",
    "nuxt": "^2.3.4",
    "puppeteer": "^1.11.0",
    "vue-jest": "^3.0.1"
